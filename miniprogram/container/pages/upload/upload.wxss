/* miniprogram/container/pages/upload/upload.wxss */
.wrap{
    padding: 24rpx;
}
.upload-btn{
    border: 2rpx solid #e4e3e3;
    width: 160rpx;
    height: 160rpx;
    text-align: center;
    line-height: 160rpx;
    font-size: 64rpx;
    color: #e4e3e3;
    margin-top: 12rpx;
}
.img-box{
    display: flex;
    flex-wrap: wrap;
}
.upload-image{
    width: 160rpx;
    height: 160rpx;
    flex-shrink: 0;
    margin: 0 12rpx;
    border: 2rpx solid #e4e3e3;
    border-radius: 4rpx;
}
.upload-image image{
    width: 100%;
    height: 100%;
}